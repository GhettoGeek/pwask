{
  "name": "spa-starter-kit",
  "version": "1.0.0",
  "description": "The most advanced starter kit for a single page application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7s4r/spa-starter-kit.git"
  },
  "author": "Gueorgui AGAPOV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/7s4r/spa-starter-kit/issues"
  },
  "homepage": "https://github.com/7s4r/spa-starter-kit#readme",
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|svg)$": "<rootDir>/tests/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "dev": "parcel index.html --https",
    "lint": "eslint src",
    "test:unit": "jest --coverage",
    "test:e2e": "testcafe chrome tests/*.js",
    "test:e2e:ci": "testcafe \"firefox:headless\" tests/*.js --app \"npm run serve\"",
    "build": "parcel build index.html",
    "start": "serve -s dist"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-static-files-copy": "^2.1.1",
    "react-test-renderer": "^16.8.6",
    "testcafe": "^1.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "i18next": "^17.0.1",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": ">= 16.3.0",
    "react-dom": ">= 16.3.0",
    "react-i18next": "^10.10.0",
    "react-router-dom": "^5.0.0",
    "serve": "^11.0.0"
  }
}
